//import com.atlassian.jira.user.util.UserManager

IssueManager im = ComponentAccessor.getIssueManager()
CustomFieldManager cfm = ComponentAccessor.getCustomFieldManager()
ApplicationUser user = ComponentAccessor.getUserManager().getUserByKey("atlassian")
//ApplicationUser user = ComponentAccessor.getJiraAuthenticationContext().getLoggedInUser()

//AutoNotificationNumber 18907
CustomField ann = cfm.getCustomFieldObjectsByName("Auto notifications number") ? cfm.getCustomFieldObjectsByName("Auto notifications number").toArray()[0] : null
Double num = (Double) issue.getCustomFieldValue(ann)
if(num!=null){
    num = num-1
    issue.setCustomFieldValue(ann, num)
}

//NotificationRemindDate 18905
CustomField nrd = cfm.getCustomFieldObjectsByName("Notification remind date") ? cfm.getCustomFieldObjectsByName("Notification remind date").toArray()[0] : null
Date olddat = (Date) issue.getCustomFieldValue(nrd)
if(olddat!=null){
    Calendar cal = Calendar.getInstance()
    cal.setTime(olddat)
    cal.add(Calendar.DAY_OF_MONTH, 2) 
    Date newDate = cal.getTime()
    issue.setCustomFieldValue(nrd, new java.sql.Timestamp(newDate.getTime()))
}

im.updateIssue(user, issue, EventDispatchOption.DO_NOT_DISPATCH, false)</arg>
