    }
    
    //Agent test 17103 - prod 17103
    def fieldAgent = ComponentAccessor.getCustomFieldManager().getCustomFieldObject(17103)
    def userManager = ComponentAccessor.getUserManager()
    
    def userAgent = userManager.getUserByName("alessandro.guidetti")
	issue.setCustomFieldValue(fieldAgent, userAgent)
   
    ComponentAccessor.getIssueManager().updateIssue(currentUser, issue, EventDispatchOption.DO_NOT_DISPATCH, false);

    issueIndexingService.reIndex(issue)
	ComponentAccessor.jiraAuthenticationContext.setLoggedInUser(oldUser)
}</arg>
