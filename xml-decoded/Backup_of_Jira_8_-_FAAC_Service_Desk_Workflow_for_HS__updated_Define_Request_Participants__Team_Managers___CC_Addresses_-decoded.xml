                  <arg name="FIELD_INLINE_SCRIPT">import com.atlassian.jira.component.ComponentAccessor
import com.atlassian.jira.issue.Issue
import com.atlassian.jira.issue.IssueManager
import com.atlassian.jira.user.util.UserUtil
import com.atlassian.jira.issue.MutableIssue
import com.atlassian.jira.jql.parser.JqlQueryParser
import org.apache.log4j.Category
import com.atlassian.jira.util.JiraUtils;
import com.atlassian.jira.issue.index.IssueIndexingService
import com.atlassian.jira.user.ApplicationUser
import com.atlassian.jira.event.type.EventDispatchOption
import com.atlassian.jira.security.groups.GroupManager
import com.atlassian.jira.issue.fields.CustomField
import com.atlassian.jira.issue.CustomFieldManager

IssueIndexingService issueIndexingService = ComponentAccessor.getComponent(IssueIndexingService)
IssueManager 		issueManager 		= ComponentAccessor.getIssueManager()
Category log = Category.getInstance("com.onresolve.jira.groovy.PostFunction")
log.setLevel(org.apache.log4j.Level.DEBUG)
//MutableIssue issue = issueManager.getIssueByKeyIgnoreCase("HS-14769")

CustomFieldManager 	customFieldManager 	= ComponentAccessor.getCustomFieldManager()

GroupManager groupManager = ComponentAccessor.getGroupManager();
Collection&lt;String&gt; groupNames = groupManager.getGroupNamesForUser(issue.getReporter().getKey());
if (groupNames.size()==0)
{
    groupNames = groupManager.getGroupNamesForUser(issue.getReporter());
}
def groupVisibility = ""
def passesCondition =false;

for( String gName : groupNames ){
    if (gName.contains("HS-Visibility-") &amp;&amp; passesCondition == false &amp;&amp; !gName.contains("HS-Visibility-CPQ")){
        passesCondition =true
        groupVisibility = gName
	}
}

/* Get Insight IQL Facade from plugin accessor */
Class iqlFacadeClass = ComponentAccessor.getPluginAccessor().getClassLoader().findClass("com.riadalabs.jira.plugins.insight.channel.external.api.facade.IQLFacade"); 
def iqlFacade = ComponentAccessor.getOSGiComponentInstanceOfType(iqlFacadeClass);
/* Get Insight Object Facade from plugin accessor */
Class objectFacadeClass = ComponentAccessor.getPluginAccessor().getClassLoader().findClass("com.riadalabs.jira.plugins.insight.channel.external.api.facade.ObjectFacade");
def objectFacade = ComponentAccessor.getOSGiComponentInstanceOfType(objectFacadeClass);

String queryParam = "\"" + groupVisibility + "\""
def objects = iqlFacade.findObjectsByIQLAndSchema(18, "objectType=\"Customer_Company\" AND \"HS_VISIBILITY_GROUP\" IN (" + queryParam + ")"); 
def company = "\"" + objects[0].getName() + "\""

//HS_BRAND test 21903 - prod 22000
CustomField fieldBrand = customFieldManager.getCustomFieldObject(Long.parseLong("22000"))
def brandValue = issue.getCustomFieldValue(fieldBrand)
def brand = "\"" + brandValue[0].getName() + "\""
def objectsCountry = iqlFacade.findObjectsByIQLAndSchema(18, "objectType=\"BKO_Users_Map\" AND \"CUSTOMER_COMPANY_REF\" = " + company + " AND \"BRAND_REF\" = (" + brand + ")"); 

//BKO_Users_Map - ASSIGNEE - test 2499 - prod 2271
def assignee = objectFacade.loadObjectAttributeBean(objectsCountry[0].getId(), 2271).getObjectAttributeValueBeans()[0].getValue()

def userManager = ComponentAccessor.getUserManager()
def user = ComponentAccessor.getUserUtil().getUserByName(assignee)
//def user = userManager.getUserByName(assignee)
issue.setAssignee(user)

//HS_COMPANY test 22503 - prod 22503
issue.setCustomFieldValue(ComponentAccessor.getCustomFieldManager().getCustomFieldObject(22503), objects)
ComponentAccessor.jiraAuthenticationContext.setLoggedInUser(ComponentAccessor.getUserManager().getUserByKey("atlassian"))
def LoggedInUser = ComponentAccessor.getJiraAuthenticationContext().getLoggedInUser()

issueManager.updateIssue(LoggedInUser, issue, EventDispatchOption.DO_NOT_DISPATCH, false)
issueIndexingService.reIndex(issue)
</arg>
